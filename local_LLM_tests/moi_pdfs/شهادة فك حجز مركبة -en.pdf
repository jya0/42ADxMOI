                    closeBtn: true, href: errorURL,
                    'onComplete': function () {
                        $('#fancybox-frame').load(function () { // wait for frame to load and then gets it's height
                            $('#fancybox-content').height($(this).contents().find('body').height() + 30);
                        });
                    }
                }); 
        }
